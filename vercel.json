{
  "version": 2,
  "builds": [
    {
      "src": "src/myproject/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
    },
    {
      "src": "src/myapp/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/src/myapp/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "src/myproject/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "myproject.settings",
    "PYTHONPATH": "src"
  }
}
